<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9ACAF1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_x="90dp"
        android:layout_y="20dp"
        android:text="유저 선택"
        android:textSize="60dp" />

    <Button
        android:id="@+id/matching_ok"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_x="131dp"
        android:layout_y="600dp"
        android:background="@drawable/btn_s"
        android:backgroundTint="#405EF8"
        android:text="등록"
        android:textAllCaps="false"
        android:textSize="15dp"
        app:backgroundTint="@android:color/holo_blue_dark" />

    <Button
        android:id="@+id/re_match"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_x="131dp"
        android:layout_y="550dp"
        android:background="@drawable/btn_s"
        android:backgroundTint="#405EF8"
        android:text="목록표시"
        android:textAllCaps="false"
        android:textSize="15dp"
        app:backgroundTint="@android:color/holo_blue_dark" />

    <ScrollView
        android:id="@+id/user_list"
        android:layout_width="360dp"
        android:layout_height="400dp"
        android:layout_x="26dp"
        android:layout_y="120dp"
        android:background="@drawable/t_login">


        <LinearLayout
            android:id="@+id/sv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:paddingTop="10dp"
            android:layout_marginTop="20dp"
            android:paddingBottom="40dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:orientation="vertical">

        </LinearLayout>
    </ScrollView>

</AbsoluteLayout>